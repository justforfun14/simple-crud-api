{
    "compilerOptions": {
      "target": "ESNext",
      "module": "Node16",
      "rootDir": "./src",
      "outDir": "./dist",
      "esModuleInterop": true,
      "strict": true,
      "skipLibCheck": true,
      "moduleResolution": "Node16",
      "noUnusedParameters": true,
      "noUnusedLocals": true,
    },
    "include": ["src/**/*"]
  }